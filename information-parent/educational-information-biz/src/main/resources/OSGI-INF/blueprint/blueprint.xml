<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:jpa="http://aries.apache.org/xmlns/jpa/v2.0.0"
           xmlns:tx="http://aries.apache.org/xmlns/transactions/v1.2.0"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
    <jpa:enable/>
    <tx:enable-annotations/>
    <reference id="schoolInfoBeanDao" interface="com.kalix.educational.information.api.dao.ISchoolInfoBeanDao"/>

    <bean id="schoolInfoBeanServiceImpl" class="com.kalix.educational.information.biz.SchoolInfoBeanServiceImpl">
        <property name="dao" ref="schoolInfoBeanDao"/>
    </bean>
    <service id="schoolInfoBeanService" ref="schoolInfoBeanServiceImpl"
             interface="com.kalix.educational.information.api.biz.ISchoolInfoBeanService"/>

</blueprint>

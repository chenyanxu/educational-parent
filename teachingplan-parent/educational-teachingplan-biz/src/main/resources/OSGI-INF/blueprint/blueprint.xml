<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:jpa="http://aries.apache.org/xmlns/jpa/v2.0.0"
           xmlns:tx="http://aries.apache.org/xmlns/transactions/v1.2.0"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
    <jpa:enable/>
    <tx:enable-annotations/>
    <reference id="lessonInfoBeanDao" interface="com.kalix.educational.teachingplan.api.dao.ILessonInfoBeanDao"/>
    <reference id="practiceInfoBeanDao" interface="com.kalix.educational.teachingplan.api.dao.IPracticeInfoBeanDao"/>
    <reference id="setProfessionalBeanDao" interface="com.kalix.educational.teachingplan.api.dao.ISetProfessionalBeanDao"/>
    <reference id="iOrganizationBeanService" interface="com.kalix.admin.core.api.biz.IOrganizationBeanService"/>

    <bean id="lessonInfoBeanServiceImpl" class="com.kalix.educational.teachingplan.biz.LessonInfoBeanServiceImpl">
        <property name="dao" ref="lessonInfoBeanDao"/>
        <property name="organizationBeanServiceImpl" ref="iOrganizationBeanService"/>
    </bean>
    <service id="lessonInfoBeanService" ref="lessonInfoBeanServiceImpl"
             interface="com.kalix.educational.teachingplan.api.biz.ILessonInfoBeanService"/>


    <bean id="practiceInfoBeanServiceImpl" class="com.kalix.educational.teachingplan.biz.PracticeInfoBeanServiceImpl">
        <property name="dao" ref="practiceInfoBeanDao"/>
        <property name="organizationBeanServiceImpl" ref="iOrganizationBeanService"/>
    </bean>
    <service id="practiceInfoBeanService" ref="practiceInfoBeanServiceImpl"
             interface="com.kalix.educational.teachingplan.api.biz.IPracticeInfoBeanService"/>


    <bean id="setProfessionalBeanServiceImpl" class="com.kalix.educational.teachingplan.biz.SetProfessionalBeanServiceImpl">
        <property name="dao" ref="setProfessionalBeanDao"/>
    </bean>
    <service id="setProfessionalBeanService" ref="setProfessionalBeanServiceImpl"
             interface="com.kalix.educational.teachingplan.api.biz.ISetProfessionalBeanService"/>

</blueprint>
